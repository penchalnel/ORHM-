package com.BasicJava;

public class Ifelseconditions {

	public static void main(String[] args) {
     int a=7;
     int b=4;
     int c=6;
	if(a>b&a>c)
	{
		System.out.print("a is greater");
	}
	else
		if(a>c)
	{
		System.out.print("b is greater");
	}
     else {
		System.out.println("c is greater");
	}
		
		}
	}
	


